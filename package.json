  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunup",
    "cf-typegen": "wrangler types --env-file '.env.example' --env-interface 'CloudflareBindings'",
    "dev": "wrangler dev",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepack": "bun run build",
    "prepare": "bun run cf-typegen",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "trailingComma": "none"
  },
  "dependencies": {
    "@scalar/hono-api-reference": "^0.9.30",
    "@scalar/openapi-to-markdown": "^0.3.11",
    "chanfana": "https://pkg.pr.new/cloudflare/chanfana@next",
    "hono": "^4.11.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@liblaf/config": "^0.1.2",
    "@types/bun": "^1.3.5",
    "bunup": "^0.16.11",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "typescript": "^5.9.3",
    "wrangler": "^4.56.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "readme": "./README.md"
