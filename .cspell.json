{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "words": [
    "bunup",
    "cioc",
    "descr",
    "direnv",
    "dotenv",
    "geoip",
    "ipapi",
    "jscoverage",
    "jspm",
    "liblaf",
    "mihomo",
    "nuxt",
    "pids",
    "remeda",
    "stricli",
    "tslog",
    "vuepress",
    "whois",
